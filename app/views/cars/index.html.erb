<h1>Listing cars</h1>

<table>
  <tr>
    <th>Title</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cars.each do |car| %>
  <tr>
    <td><%= car.title %></td>
    <td><%= link_to 'Show', car, remote: true %></td>
    <td><%= link_to 'Edit', edit_car_path(car), remote: true %></td>
    <td><%= link_to 'Destroy', car, confirm: 'Are you sure?', method: :delete, remote: true %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Car', new_car_path, remote: true %>
